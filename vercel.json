{
    "builds": [{
        "src": "emailchecker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python 3.9" }
    },
    {
        "src": "build_file.sh",
        "use":"@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }
    }
],
"routes": [

    {
        "src": "/static/(.*)",
        "dest":"/static/$1"
    },
        {
            "src": "/(.*)",
            "dest": "emailchecker/wsgi.py"
        }
    ]
}